<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.setswana101">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Colors"
           android:parentActivityName=".MainActivity" android:label="Colors"/>
        <activity android:name=".Food"
            android:parentActivityName=".MainActivity" android:label="Food"/>
        <activity android:name=".Greetings"
            android:parentActivityName=".MainActivity" android:label="Greetings"/>
        <activity android:name=".Numbers"
            android:parentActivityName=".MainActivity" android:label="Numbers"/>
        <activity android:name=".Family"
            android:parentActivityName=".MainActivity" android:label="Family"></activity>
    </application>

</manifest>